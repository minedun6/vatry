    <script src="{{ asset('plugins/slicknav/dist/jquery.slicknav.min.js') }}"></script>
    <script src="{{ asset('app/js/wow.min.js') }}"></script>
    <script src="{{ asset('app/js/search.js') }}"></script>
    <script src="{{ asset('app/js/scripts.js') }}"></script>
    <script src="{{ asset('plugins/sifter/sifter.min.js') }}"></script>
    <script src="{{ asset('plugins/microplugin/src/microplugin.js') }}"></script>
    <script src="{{ asset('plugins/selectize/dist/js/selectize.min.js') }}"></script>
    <script src="{{ asset('app/js/Minimal-Sliding-Timer-Picker-Plugin-For-jQuery-mtimepicker/Minimal-Sliding-Timer-Picker-Plugin-For-jQuery-mtimepicker/mtimepicker.js') }}"></script>

    <!-- Bootstrap TEST -->
    <script src="{{ asset('plugins/bootstrap/dist/js/bootstrap.min.js') }}"></script>

    <script src="{{ asset('plugins/moment/min/moment.min.js') }}"></script>

    <script src="{{ asset('app/js/common.js') }}"></script>
    <script src="{{ asset('app/js/general.js') }}"></script>
    <script src="{{ asset('plugins/datatables.net/js/jquery.dataTables.min.js') }}"></script>

    <script src="https://maps.google.com/maps/api/js"></script>
    <script src="{{ asset('plugins/gmaps/gmaps.min.js') }}"></script>

    <script src="{{ asset('app/js/flights.js') }}"></script>

{% endblock %}

</body>
</html>